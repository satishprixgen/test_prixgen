<!-- place snippets -->
<template id="place_into_bar" inherit_id="website.snippets" name="Place into bar">
  <xpath expr="//div[@id='snippet_structure']/div[@class='o_panel_body']" position="inside">
    <t t-snippet="theme_prixgen.snippet_testimonial"
       t-thumbnail="/theme_prixgen/static/src/img/ui/snippet_thumb.jpg"/>
    <t t-snippet="theme_prixgen.offers_section"
       t-thumbnail="/theme_prixgen/static/src/img/ui/offers.jpeg"/>

  </xpath>
</template>
<template id="place_divider" inherit_id="website.snippets" name="Place divider">
  <xpath expr="//div[@id='snippet_structure']/div[@class='o_panel_body']" position="inside">
    <t t-snippet="theme_prixgen.snippet_divider"
       t-thumbnail="/theme_prixgen/static/src/img/ui/divider_black.png"/>
  </xpath>
</template>
<template id="client_logo-place" inherit_id="website.snippets" name="place client logos">
